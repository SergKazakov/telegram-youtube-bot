{
  "private": true,
  "scripts": {
    "start": "node --experimental-specifier-resolution=node -r dotenv-safe/config src/index.mjs",
    "dev": "nodemon --watch src --inspect --experimental-specifier-resolution=node -r dotenv-safe/config src/index.mjs",
    "test": "eslint --ignore-path .gitignore --fix --ext mjs ."
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:import/recommended",
      "xo/esnext",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ],
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/no-null": "off",
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "import/order": [
        "error",
        {
          "newlines-between": "always"
        }
      ]
    }
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-xo": "^0.33.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-unicorn": "^24.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.5.0",
    "dayjs": "^1.9.7",
    "dotenv-safe": "^8.2.0",
    "dumper.js": "^1.3.1",
    "fast-xml-parser": "^3.17.5",
    "googleapis": "^66.0.0",
    "he": "^1.2.0",
    "ioredis": "^4.19.4",
    "mongoose": "^5.11.8",
    "pubsubhubbub": "^1.0.1",
    "telegraf": "^3.38.0",
    "yup": "^0.32.8"
  },
  "engines": {
    "node": "12"
  }
}
