{
  "private": true,
  "scripts": {
    "start": "node -r dotenv-safe/config src/index.mjs",
    "dev": "nodemon --watch src --inspect -r dotenv-safe/config src/index.mjs"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:import/recommended",
      "xo",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ],
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/no-null": "off",
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "import/order": [
        "error",
        {
          "newlines-between": "always"
        }
      ],
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "^node:"
          ]
        }
      ]
    }
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.10.2",
    "dayjs": "^1.11.3",
    "dotenv-safe": "^8.2.0",
    "fast-xml-parser": "^4.0.8",
    "googleapis": "^103.0.0",
    "he": "^1.2.0",
    "ioredis": "^5.0.6",
    "mongoose": "^6.4.0",
    "pubsubhubbub": "^1.0.1",
    "telegraf": "^4.8.5",
    "yup": "^0.32.11"
  },
  "engines": {
    "node": "14"
  }
}
